<div class="switchBtn"><i class="fa fa-photo"></i></div>
<div class="imgPanel">
	<div class="field">
	  <div class="file is-warning is-boxed">
	    <label class="file-label w100">
	      <input class="file-input" type="file" name="resume" id="file1">
	      <span class="file-cta">
	        <span class="file-icon">
	          <i class="fa fa-upload"></i>
	        </span>
	        <span class="file-label">
	          上传图片
	        </span>
	      </span>
	    </label>
	  </div>
	</div>
	<!-- 上传按钮 -->
	<hr>
	<div id="cardList">
		<script type="text/html" id="cardTpl">
		{{each}}
		<div class="card mb-20">
			<div class="card-image">
				<figure class="image">
					<img src="{{$value.imgUrl}}" alt="Placeholder image">
				</figure>
			</div>
			<footer class="card-footer">
				<p class="card-footer-item"><span class="cus-p">删除</span></p>
				<p class="card-footer-item"><span class="cus-p">复制</span></p>
			</footer>
		</div>
		{{/each}}
		</script>
	</div>	
</div>